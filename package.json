{
  "name": "@solsplits/solsplits-sdk-ts",
  "version": "0.0.5",
  "description": "Typescript Client for the SolSplits Protocol",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "test": "mocha",
    "semantic-release": "semantic-release",
    "prepack": "npm run build"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/solsplits/solsplits-sdk-ts.git"
  },
  "keywords": [
    "solsplits",
    "solana",
    "sdk",
    "tools",
    "protocol",
    "npm",
    "javascript",
    "typescript",
    "esm",
    "cjs",
    "nodejs",
    "commonjs",
    "ecmascript"
  ],
  "author": "SolSplits.xyz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/solsplits/solsplits-sdk-ts/issues"
  },
  "homepage": "https://solsplits.xyz",
  "devDependencies": {
    "@coral-xyz/borsh": "^0.27.0",
    "@solana/web3.js": "^1.75.0",
    "@types/bn.js": "^5.1.1",
    "@types/chai": "^4.3.3",
    "bn.js": "^5.2.1",
    "chai": "^4.3.6",
    "semantic-release": "^19.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
